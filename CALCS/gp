set term postscript eps color enhanced blacktext 'Helvetica,24'
set output 'view_config.eps'
set yrange [0:1]
set xrange [0:20]
set ylabel 'energy / eV'
set xlabel 'configuration / -'
set key outside
plot 0.0 w l lc rgb 'black' notitle, \
 'zero.configuration.1.txt' u 0:1 w lp pt 6 title '1', \
